	<Attributes>
		<Attribute name="Список" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ РАЗЛИЧНЫЕ
	Предприятия.Наименование,
	Предприятия.Код,
	ВложенныйЗапрос.ПометкаУдаления,
	ВложенныйЗапрос.Предопределенный,
	ВложенныйЗапрос.Родитель,
	ВложенныйЗапрос.ПрефиксПредприятия,
	ВложенныйЗапрос.ТипПредприятия,
	ВложенныйЗапрос.Дивизионер,
	ВложенныйЗапрос.Учет,
	ВложенныйЗапрос.ГД,
	ВложенныйЗапрос.АльтНаименование,
	ВложенныйЗапрос.Финансист,
	ВложенныйЗапрос.Учетчик,
	ВложенныйЗапрос.Директор,
	ВложенныйЗапрос.УчетПоПодразделениям,
	ВложенныйЗапрос.ВидДеятельности,
	ВложенныйЗапрос.ДатаЗакрытияУчет,
	ВложенныйЗапрос.КорпоративныйEMail,
	ВложенныйЗапрос.НеУчитыватьВОтчетахГД,
	ВложенныйЗапрос.ЕстьПодчиненные,
	ВложенныйЗапрос.ПроцентнаяСтавка,
	ВложенныйЗапрос.ГруппировкаМСФО,
	Предприятия.Ссылка
ИЗ
	Справочник.Предприятия КАК Предприятия
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ (ВЫБРАТЬ РАЗЛИЧНЫЕ
			СправочникПредприятия.Ссылка КАК Ссылка,
			СправочникПредприятия.ПометкаУдаления КАК ПометкаУдаления,
			СправочникПредприятия.Предопределенный КАК Предопределенный,
			СправочникПредприятия.Родитель КАК Родитель,
			СправочникПредприятия.Код КАК Код,
			СправочникПредприятия.Наименование КАК Наименование,
			СправочникПредприятия.ПрефиксПредприятия КАК ПрефиксПредприятия,
			СправочникПредприятия.ТипПредприятия КАК ТипПредприятия,
			ОсновныеДолжностиПредприятияСрезПоследних.Сотрудник КАК Дивизионер,
			СправочникПредприятия.Учет КАК Учет,
			СправочникПредприятия.ГД КАК ГД,
			СправочникПредприятия.АльтНаименование КАК АльтНаименование,
			ОсновныеДолжностиПредприятияСрезПоследних8.Сотрудник КАК Финансист,
			ОсновныеДолжностиПредприятияСрезПоследних7.Сотрудник КАК Учетчик,
			ОсновныеДолжностиПредприятияСрезПоследних1.Сотрудник КАК Директор,
			СправочникПредприятия.УчетПоПодразделениям КАК УчетПоПодразделениям,
			СправочникПредприятия.ВидДеятельности КАК ВидДеятельности,
			СправочникПредприятия.ДатаЗакрытияУчет КАК ДатаЗакрытияУчет,
			СправочникПредприятия.КорпоративныйEMail КАК КорпоративныйEMail,
			СправочникПредприятия.НеУчитыватьВОтчетахГД КАК НеУчитыватьВОтчетахГД,
			ВЫБОР
				КОГДА СправочникПредприятия.Ссылка В
						(ВЫБРАТЬ
							ВЫБОР
								КОГДА Предприятия.Родитель = ЗНАЧЕНИЕ(Справочник.предприятия.ПустаяСсылка)
									ТОГДА NULL
								ИНАЧЕ Предприятия.Родитель
							КОНЕЦ КАК Родитель
						ИЗ
							Справочник.Предприятия КАК Предприятия)
					ТОГДА ИСТИНА
				ИНАЧЕ ЛОЖЬ
			КОНЕЦ КАК ЕстьПодчиненные,
			КЗ_ПроцентнаяСтавкаПредприятияСрезПоследних.ПроцентнаяСтавка КАК ПроцентнаяСтавка,
			СправочникПредприятия.ГруппировкаМСФО КАК ГруппировкаМСФО
		ИЗ
			Справочник.Предприятия КАК СправочникПредприятия
				ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ОсновныеДолжностиПредприятия КАК ОсновныеДолжностиПредприятияСрезПоследних
				ПО СправочникПредприятия.Ссылка = ОсновныеДолжностиПредприятияСрезПоследних.Предприятие
					И (ОсновныеДолжностиПредприятияСрезПоследних.Должность = ЗНАЧЕНИЕ(Перечисление.ОсновныеДолжностиПредприятия.Дивизионер))
					И (ОсновныеДолжностиПредприятияСрезПоследних.Подразделение = ЗНАЧЕНИЕ(Справочник.СтруктураПредприятия.ПустаяСсылка))
				ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ОсновныеДолжностиПредприятия КАК ОсновныеДолжностиПредприятияСрезПоследних8
				ПО СправочникПредприятия.Ссылка = ОсновныеДолжностиПредприятияСрезПоследних8.Предприятие
					И (ОсновныеДолжностиПредприятияСрезПоследних8.Должность = ЗНАЧЕНИЕ(Перечисление.ОсновныеДолжностиПредприятия.Финансист))
					И (ОсновныеДолжностиПредприятияСрезПоследних8.Подразделение = ЗНАЧЕНИЕ(Справочник.СтруктураПредприятия.ПустаяСсылка))
				ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ОсновныеДолжностиПредприятия КАК ОсновныеДолжностиПредприятияСрезПоследних7
				ПО СправочникПредприятия.Ссылка = ОсновныеДолжностиПредприятияСрезПоследних7.Предприятие
					И (ОсновныеДолжностиПредприятияСрезПоследних7.Должность = ЗНАЧЕНИЕ(Перечисление.ОсновныеДолжностиПредприятия.Учетчик))
					И (ОсновныеДолжностиПредприятияСрезПоследних7.Подразделение = ЗНАЧЕНИЕ(Справочник.СтруктураПредприятия.ПустаяСсылка))
				ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ОсновныеДолжностиПредприятия КАК ОсновныеДолжностиПредприятияСрезПоследних1
				ПО СправочникПредприятия.Ссылка = ОсновныеДолжностиПредприятияСрезПоследних1.Предприятие
					И (ОсновныеДолжностиПредприятияСрезПоследних1.Должность = ЗНАЧЕНИЕ(Перечисление.ОсновныеДолжностиПредприятия.РуководительПроекта))
					И (ОсновныеДолжностиПредприятияСрезПоследних1.Подразделение = ЗНАЧЕНИЕ(Справочник.СтруктураПредприятия.ПустаяСсылка))
				ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.КЗ_ПроцентнаяСтавкаПредприятия.СрезПоследних(&amp;Период, ) КАК КЗ_ПроцентнаяСтавкаПредприятияСрезПоследних
				ПО (КЗ_ПроцентнаяСтавкаПредприятияСрезПоследних.Предприятие = СправочникПредприятия.Ссылка)
				ВНУТРЕННЕЕ СОЕДИНЕНИЕ Справочник.Предприятия КАК Предприятия
				ПО СправочникПредприятия.Ссылка = Предприятия.Ссылка
					И (СправочникПредприятия.Ссылка В ИЕРАРХИИ
							(ВЫБРАТЬ
								Предприятия1.Родитель
							ИЗ
								Справочник.Предприятия КАК Предприятия1
							ГДЕ
								Предприятия1.Ссылка В (&amp;ДоступныеПредприятия))
						ИЛИ ЛОЖЬ В
							(ВЫБРАТЬ
								Пользователи.ДоступныПредприятияИзСписка
							ИЗ
								Справочник.Пользователи КАК Пользователи
							ГДЕ
								Пользователи.Ссылка В (&amp;ДоступныеПользователи)))
) КАК ВложенныйЗапрос
		ПО Предприятия.Ссылка = ВложенныйЗапрос.Ссылка</QueryText>
