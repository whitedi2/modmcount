				<QueryText>ВЫБРАТЬ
	ДокументД_Бюджет.Ссылка,
	ДокументД_Бюджет.ПометкаУдаления,
	ДокументД_Бюджет.Номер,
	ДокументД_Бюджет.Дата,
	ДокументД_Бюджет.Проведен,
	ДокументД_Бюджет.Предприятие,
	ДокументД_Бюджет.Сценарий,
	ДокументД_Бюджет.Комментарий,
	ДокументД_Бюджет.Месяц,
	ДокументД_Бюджет.ВсеМесяцы,
	ДокументД_Бюджет.Файл,
	ДокументД_Бюджет.СценарийПП,
	ДокументД_Бюджет.МоментВремени,
	Д_ЗаписанныеДокументы.ДатаРедакции КАК ДатаРедакции,
	Д_ЗаписанныеДокументы.Редактор КАК Редактор,
	ВЫБОР
		КОГДА ДокументД_Бюджет.Номер В (&amp;ФонЗадания)
			ТОГДА "Проведение..."
		ИНАЧЕ ВЫБОР
				КОГДА Д_ЗаписанныеДокументы.СтатусПроведения = ""
					ТОГДА "Готово"
				ИНАЧЕ Д_ЗаписанныеДокументы.СтатусПроведения
			КОНЕЦ
	КОНЕЦ КАК ФоновоеПроведение,
	ЕСТЬNULL(Б_ПараметрыБюджета.Утвержден, ЛОЖЬ) КАК Утвержден,
	ЕСТЬNULL(Б_ПараметрыБюджета.ПризнакРедактирования, ЛОЖЬ) КАК ЗапретРедактирования
ИЗ
	Документ.Д_Бюджет КАК ДокументД_Бюджет
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.Д_ЗаписанныеДокументы КАК Д_ЗаписанныеДокументы
		ПО (Д_ЗаписанныеДокументы.ПредставлениеДокумента = ДокументД_Бюджет.Ссылка)
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.Б_ПараметрыБюджета КАК Б_ПараметрыБюджета
		ПО ДокументД_Бюджет.Предприятие = Б_ПараметрыБюджета.Предприятие
			И ДокументД_Бюджет.Сценарий = Б_ПараметрыБюджета.Сценарий
ГДЕ
	ВЫБОР
			КОГДА &amp;БюджетнаяРоль = ЗНАЧЕНИЕ(Перечисление.Д_ТипыБюджетов.БюджетЗакупок)
					ИЛИ &amp;БюджетнаяРоль = ЗНАЧЕНИЕ(Перечисление.Д_ТипыБюджетов.БюджетПродаж)
				ТОГДА ДокументД_Бюджет.БезБюджетаПроизводства = ЛОЖЬ
						ИЛИ ДокументД_Бюджет.Предприятие В (&amp;Предприятие)
			ИНАЧЕ ДокументД_Бюджет.Предприятие В (&amp;Предприятие)
		КОНЕЦ

СГРУППИРОВАТЬ ПО
	ДокументД_Бюджет.Ссылка,
	ДокументД_Бюджет.ПометкаУдаления,
	ДокументД_Бюджет.Номер,
	ДокументД_Бюджет.Дата,
	ДокументД_Бюджет.Проведен,
	ДокументД_Бюджет.Предприятие,
	ДокументД_Бюджет.Сценарий,
	ДокументД_Бюджет.Комментарий,
	ДокументД_Бюджет.Месяц,
	ДокументД_Бюджет.ВсеМесяцы,
	ДокументД_Бюджет.Файл,
	ДокументД_Бюджет.СценарийПП,
	ДокументД_Бюджет.МоментВремени,
	Д_ЗаписанныеДокументы.ДатаРедакции,
	Д_ЗаписанныеДокументы.Редактор,
	ВЫБОР
		КОГДА ДокументД_Бюджет.Номер В (&amp;ФонЗадания)
			ТОГДА "Проведение..."
		ИНАЧЕ ВЫБОР
				КОГДА Д_ЗаписанныеДокументы.СтатусПроведения = ""
					ТОГДА "Готово"
				ИНАЧЕ Д_ЗаписанныеДокументы.СтатусПроведения
			КОНЕЦ
	КОНЕЦ,
	ЕСТЬNULL(Б_ПараметрыБюджета.Утвержден, ЛОЖЬ),
	ЕСТЬNULL(Б_ПараметрыБюджета.ПризнакРедактирования, ЛОЖЬ)</QueryText>
