			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ
	ДокументУЧ_ДвижениеДС.Ссылка,
	ДокументУЧ_ДвижениеДС.ВерсияДанных,
	ДокументУЧ_ДвижениеДС.ПометкаУдаления,
	ДокументУЧ_ДвижениеДС.Номер,
	ДокументУЧ_ДвижениеДС.Дата,
	ДокументУЧ_ДвижениеДС.Проведен,
	ДокументУЧ_ДвижениеДС.Предприятие,
	ВЫБОР
		КОГДА ДокументУЧ_ДвижениеДС.ПриходРасход = "Приход"
			ТОГДА ДокументУЧ_ДвижениеДС.СуммаДокумента
		ИНАЧЕ 0
	КОНЕЦ КАК Приход,
	ДокументУЧ_ДвижениеДС.Комментарий,
	ДокументУЧ_ДвижениеДС.Подразделение,
	ДокументУЧ_ДвижениеДС.Счет,
	ДокументУЧ_ДвижениеДС.Субконто1,
	ДокументУЧ_ДвижениеДС.Субконто2,
	ДокументУЧ_ДвижениеДС.Субконто3,
	ДокументУЧ_ДвижениеДС.Валюта1,
	ДокументУЧ_ДвижениеДС.Курс1,
	ДокументУЧ_ДвижениеДС.ПроводитьВКазне,
	ДокументУЧ_ДвижениеДС.ПриходРасход,
	ДокументУЧ_ДвижениеДС.ВалютнаяСумма1,
	ДокументУЧ_ДвижениеДС.Проводки.(
		Ссылка,
		НомерСтроки,
		КорСчет,
		КорСубконто1,
		КорСубконто2,
		КорСубконто3,
		Сумма,
		Содержание,
		Валюта,
		Курс,
		ВалСумма
	),
	ДокументУЧ_ДвижениеДС.МоментВремени,
	ДокументУЧ_ДвижениеДС.Содержание,
	ДокументУЧ_ДвижениеДС.Контрагент,
	ВЫБОР
		КОГДА ДокументУЧ_ДвижениеДС.ПриходРасход = "Расход"
			ТОГДА ДокументУЧ_ДвижениеДС.СуммаДокумента
		ИНАЧЕ 0
	КОНЕЦ КАК Расход,
	ДокументУЧ_ДвижениеДС.СуммаДокумента,
	ДокументУЧ_ДвижениеДС.ДокОснование,
	ДокументУЧ_ДвижениеДС.ДокОснование2
ИЗ
	Документ.УЧ_ДвижениеДС КАК ДокументУЧ_ДвижениеДС
ГДЕ
	ДокументУЧ_ДвижениеДС.Предприятие В(&amp;Предприятия)
	И ВЫБОР
			КОГДА &amp;ДокОснование ЕСТЬ NULL 
				ТОГДА ИСТИНА
			ИНАЧЕ ДокументУЧ_ДвижениеДС.ДокОснование = &amp;ДокОснование
		КОНЕЦ</QueryText>
				<MainTable>Document.УЧ_ДвижениеДС</MainTable>
				<ListSettings>
					<dcsset:filter>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>dfcece9d-5077-440b-b6b3-45a5cb4538eb</dcsset:userSettingID>
					</dcsset:filter>
					<dcsset:order>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>88619765-ccb3-46c6-ac52-38e9c992ebd4</dcsset:userSettingID>
					</dcsset:order>
					<dcsset:conditionalAppearance>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>b75fecce-942b-4aed-abc9-e6a02e460fb3</dcsset:userSettingID>
					</dcsset:conditionalAppearance>
					<dcsset:itemsViewMode>Normal</dcsset:itemsViewMode>
					<dcsset:itemsUserSettingID>911b6018-f537-43e8-a417-da56b22f9aec</dcsset:itemsUserSettingID>
