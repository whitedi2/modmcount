	Соответствия.Вставить(Справочники.Номенклатура.ПустаяСсылка(), ?(ЭтоГП, ПланыСчетов.Учетный.Счет43(), ПланыСчетов.Учетный.Счет1001()));
	
	МассивМатериаловБезСчета = Новый Массив;
