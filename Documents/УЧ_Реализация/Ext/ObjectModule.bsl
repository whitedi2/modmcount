			ТекСтрока.Счет = ТекСтрока.Номенклатура.Счет10;		
		КонецЕсли;
	КонецЦикла; 
	
	
	ДоставкаНаЕдиницу = Истина;
	СуммаИтог = Товары.Итог("СуммаОтгрузки") + Услуги.Итог("СуммаОтгрузки");
