			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ
	ДокументУЧ_Реализация.Ссылка,
	ДокументУЧ_Реализация.ПометкаУдаления,
	ДокументУЧ_Реализация.Номер,
	ДокументУЧ_Реализация.Дата,
	ДокументУЧ_Реализация.Проведен,
	ДокументУЧ_Реализация.Предприятие,
	ДокументУЧ_Реализация.Комментарий,
	ДокументУЧ_Реализация.Контрагент,
	ДокументУЧ_Реализация.Счет,
	ДокументУЧ_Реализация.ТратыНалива,
	ДокументУЧ_Реализация.Акциз,
	ДокументУЧ_Реализация.ТратыНаливаКРХ,
	ДокументУЧ_Реализация.Договор,
	ДокументУЧ_Реализация.ДоставкаНаЕдиницу,
	ДокументУЧ_Реализация.ПредприятиеПоставщик,
	ДокументУЧ_Реализация.ДоговорВн,
	ДокументУЧ_Реализация.Флаг60_79,
	ДокументУЧ_Реализация.ВидДеятельности,
	ДокументУЧ_Реализация.НалогиИСборыАкциз,
	ДокументУЧ_Реализация.НачислятьДоставку,
	ДокументУЧ_Реализация.ДокументОснование,
	ДокументУЧ_Реализация.НеСоздавать,
	ДокументУЧ_Реализация.НачислятьВерх,
	ДокументУЧ_Реализация.ДатыНеСовпадают,
	ДокументУЧ_Реализация.ДатаОтгрузки,
	ДокументУЧ_Реализация.ФлагВалюты,
	ДокументУЧ_Реализация.Валюта,
	ДокументУЧ_Реализация.Курс,
	ДокументУЧ_Реализация.ДокОснование,
	ДокументУЧ_Реализация.ФлагВозврат,
	ДокументУЧ_Реализация.ВидВерха,
	ДокументУЧ_Реализация.ЮрЛицо,
	ДокументУЧ_Реализация.СуммаВключаетНДС,
	ДокументУЧ_Реализация.УчитыватьНДС,
	ДокументУЧ_Реализация.Автор,
	ДокументУЧ_Реализация.Редактор,
	ДокументУЧ_Реализация.СуммаДокумента,
	ДокументУЧ_Реализация.МоментВремени,
	УчетныйОстатки.СуммаОстатокДт,
	ВЫБОР
		КОГДА ЕСТЬNULL(УчетныйОстатки.СуммаОстатокДт, 0) = 0
			ТОГДА "Закрыт"
		ИНАЧЕ ""
	КОНЕЦ КАК Признак,
	ЕСТЬNULL(УчетныйОстатки.СуммаОстатокДт, 0) КАК Остаток
ИЗ
	Документ.УЧ_Реализация КАК ДокументУЧ_Реализация
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрБухгалтерии.Учетный.Остатки(&amp;ДатаДокумента, Счет = ЗНАЧЕНИЕ(ПланСчетов.Учетный.ПокупателиОсн), , ) КАК УчетныйОстатки
		ПО ДокументУЧ_Реализация.Ссылка = УчетныйОстатки.Субконто3
ГДЕ
	ДокументУЧ_Реализация.Предприятие В(&amp;ДоступныеПредприятия)
	И ВЫБОР
			КОГДА &amp;Контрагент = НЕОПРЕДЕЛЕНО
				ТОГДА ИСТИНА
			ИНАЧЕ ДокументУЧ_Реализация.Контрагент = &amp;Контрагент
		КОНЕЦ
	И ВЫБОР
			КОГДА &amp;Договор = НЕОПРЕДЕЛЕНО
				ТОГДА ИСТИНА
			ИНАЧЕ ДокументУЧ_Реализация.Договор = &amp;Договор
		КОНЕЦ
	И ВЫБОР
			КОГДА &amp;ПоказатьЗакрытыеОтгрузки = ЛОЖЬ
				ТОГДА НЕ УчетныйОстатки.СуммаОстатокДт ЕСТЬ NULL 
			ИНАЧЕ ИСТИНА
		КОНЕЦ</QueryText>
