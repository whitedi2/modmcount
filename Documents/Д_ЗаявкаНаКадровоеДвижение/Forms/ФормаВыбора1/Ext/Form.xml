	<Attributes>
		<Attribute name="Список" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ
	Д_ЗаявкаНаКадровоеДвижение.Дата КАК Дата,
	Д_ЗаявкаНаКадровоеДвижение.Ссылка,
	Д_ЗаявкаНаКадровоеДвижение.Предприятие,
	Д_ТекущаяЗадача.ТекущаяЗадача КАК Задача,
	Д_ТекущаяЗадача.Исполнители КАК Исполнитель,
	Д_ЗаявкаНаКадровоеДвижение.Комментарий,
	Д_ЗаявкаНаКадровоеДвижение.Автор,
	Д_ЗаявкаНаКадровоеДвижение.Номер,
	Д_ТекущаяЗадача.ТочкаМаршрута,
	Д_ЗаявкаНаКадровоеДвижение.Подразделение,
	ВЫБОР
		КОГДА Д_ТекущаяЗадача.ТочкаМаршрута = ЗНАЧЕНИЕ(БизнесПроцесс.Согласование2.ТочкаМаршрута.Действие5)
				ИЛИ Д_ТекущаяЗадача.ТочкаМаршрута = ЗНАЧЕНИЕ(БизнесПроцесс.Согласование3.ТочкаМаршрута.Действие5)
				ИЛИ Д_ТекущаяЗадача.ТочкаМаршрута = ЗНАЧЕНИЕ(БизнесПроцесс.Согласование3.ТочкаМаршрута.Действие6)
			ТОГДА "НаИсполнении"
		ИНАЧЕ ВЫБОР
				КОГДА Д_ТекущаяЗадача.БизнесПроцессЗавершен = ИСТИНА
					ТОГДА "Исполнено"
				ИНАЧЕ ""
			КОНЕЦ
	КОНЕЦ КАК Стадия,
	Д_ЗаявкаНаКадровоеДвижение.ВидСЗ,
	Д_ЗаявкаНаКадровоеДвижение.Кому,
	Д_ЗаявкаНаКадровоеДвижение.Комментарий КАК Комментарий1
ИЗ
	Документ.Д_ЗаявкаНаКадровоеДвижение КАК Д_ЗаявкаНаКадровоеДвижение
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.Д_ТекущаяЗадача КАК Д_ТекущаяЗадача
		ПО Д_ЗаявкаНаКадровоеДвижение.Ссылка = Д_ТекущаяЗадача.Документ
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			ВложенныйЗапрос.Документ КАК Документ
		ИЗ
			(ВЫБРАТЬ
				Д_ДоступностьДокументов.Документ КАК Документ
			ИЗ
				РегистрСведений.Д_ДоступностьДокументов КАК Д_ДоступностьДокументов
			ГДЕ
				(ВЫБОР
							КОГДА &amp;Согласовано
								ТОГДА ВЫРАЗИТЬ(Д_ДоступностьДокументов.Значение КАК Справочник.Пользователи) В (&amp;ТекущийПользователь)
										И Д_ДоступностьДокументов.Признак = "Согласование"
							ИНАЧЕ ЛОЖЬ
						КОНЕЦ
						ИЛИ ВЫБОР
							КОГДА &amp;Оплачено
								ТОГДА ВЫРАЗИТЬ(Д_ДоступностьДокументов.Значение КАК Справочник.Пользователи) В (&amp;ТекущийПользователь)
										И Д_ДоступностьДокументов.Признак = "Ознакомление"
							ИНАЧЕ ЛОЖЬ
						КОНЕЦ
						ИЛИ ВЫБОР
							КОГДА &amp;ПринадлежностьПредприятию
								ТОГДА ВЫБОР
											КОГДА &amp;ВсеПредприятия
												ТОГДА ИСТИНА
											ИНАЧЕ ВЫРАЗИТЬ(Д_ДоступностьДокументов.Значение КАК Справочник.Предприятия) В (&amp;ДоступныеПредприятия)
										КОНЕЦ
										И ВЫБОР
											КОГДА &amp;ВсеПодразделения
													ИЛИ (ВЫРАЗИТЬ(Д_ДоступностьДокументов.Значение КАК Справочник.Предприятия)) = ЛОЖЬ
												ТОГДА ИСТИНА
											ИНАЧЕ ВЫРАЗИТЬ(Д_ДоступностьДокументов.Значение2 КАК Справочник.СтруктураПредприятия) В (&amp;ДоступныеПодразделения)
										КОНЕЦ
							ИНАЧЕ ЛОЖЬ
						КОНЕЦ
						ИЛИ ВЫБОР
							КОГДА &amp;Исполнено
