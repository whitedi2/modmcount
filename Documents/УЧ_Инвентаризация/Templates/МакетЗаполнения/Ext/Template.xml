		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	СпрНоменклатура.Ссылка КАК Номенклатура,
	СУММА(ЕСТЬNULL(УчетныйОстатки.КоличествоОстаток, 0)) КАК Количество,
	СУММА(0) КАК КоличествоПрошлаяИнвПодснятие,
	СУММА(ЕСТЬNULL(УчетныйОстатки.СуммаОстаток, 0)) КАК Сумма,
	СпрНоменклатура.ВидНоменклатуры КАК НомГруппа,
	СУММА(0) КАК СуммаПрошлаяИнвПодснятие,
	ЕСТЬNULL(УчетныйОстатки.Субконто2, УчетныйОстатки1.Субконто2) КАК Склад,
	СУММА(ЕСТЬNULL(УчетныйОстатки1.КоличествоОстаток, 0)) КАК КоличествоБУ,
	СУММА(ЕСТЬNULL(УчетныйОстатки1.СуммаОстаток, 0)) КАК СуммаБУ,
	УчетныйОстатки.Субконто3 КАК СерияНоменклатуры
ПОМЕСТИТЬ ВТ_Остатки
ИЗ
	Справочник.Номенклатура КАК СпрНоменклатура
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрБухгалтерии.Учетный.Остатки(
				&amp;ДатаОстатков,
				Счет В ИЕРАРХИИ (&amp;СчетаУчета),
				,
				(&amp;Склад = ЗНАЧЕНИЕ(Справочник.Склады.ПустаяСсылка)
					ИЛИ Субконто2 = &amp;Склад)
					И Предприятия = &amp;Предприятие
					И (&amp;НеОтбиратьПоНоменклатуре
						ИЛИ Субконто1 В (&amp;СписокНоменклатур))) КАК УчетныйОстатки
		ПО СпрНоменклатура.Ссылка = УчетныйОстатки.Субконто1
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрБухгалтерии.Хозрасчетный.Остатки(
				&amp;ДатаОстатков,
				Счет В ИЕРАРХИИ (&amp;СчетаУчетаБУ),
				,
				(&amp;Склад = ЗНАЧЕНИЕ(Справочник.Склады.ПустаяСсылка)
					ИЛИ Субконто2 = &amp;Склад
					ИЛИ Субконто2 ЕСТЬ NULL)
					И Организация = &amp;Организация
					И (&amp;НеОтбиратьПоНоменклатуре
						ИЛИ Субконто1 В (&amp;СписокНоменклатур))) КАК УчетныйОстатки1
		ПО СпрНоменклатура.Ссылка = УчетныйОстатки1.Субконто1
ГДЕ
	НЕ СпрНоменклатура.ЭтоГруппа
	И ВЫБОР
			КОГДА &amp;ГруппаНоменклатуры = "Все группы"
				ТОГДА ИСТИНА
			ИНАЧЕ СпрНоменклатура.Ссылка В ИЕРАРХИИ (&amp;ГруппаНоменклатуры)
		КОНЕЦ
	И (НЕ УчетныйОстатки.Субконто1 ЕСТЬ NULL
			ИЛИ НЕ УчетныйОстатки1.Субконто1 ЕСТЬ NULL
			ИЛИ &amp;ВыводитьТоварСНулевымОстатком)
	И (&amp;НеОтбиратьПоНоменклатуре
			ИЛИ СпрНоменклатура.Ссылка В (&amp;СписокНоменклатур))

СГРУППИРОВАТЬ ПО
	СпрНоменклатура.ВидНоменклатуры,
	СпрНоменклатура.Ссылка,
	ЕСТЬNULL(УчетныйОстатки.Субконто2, УчетныйОстатки1.Субконто2),
	УчетныйОстатки.Субконто3
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВремТЧ.Номенклатура КАК Номенклатура,
	ВремТЧ.Количество КАК Количество,
	ВремТЧ.КоличествоПрошлаяИнвПодснятие КАК КоличествоПрошлаяИнвПодснятие,
	ВремТЧ.Сумма КАК Сумма,
	ВремТЧ.НомГруппа КАК НомГруппа,
	ВремТЧ.СуммаПрошлаяИнвПодснятие КАК СуммаПрошлаяИнвПодснятие,
	ЕСТЬNULL(ЦеныНоменклатурыСрезПоследних.Цена, 0) КАК ЦенаЗакупочная,
	ВремТЧ.Склад КАК Склад,
	ВремТЧ.КоличествоБУ КАК КоличествоБУ,
	ВремТЧ.СуммаБУ КАК СуммаБУ,
	ВремТЧ.СерияНоменклатуры КАК СерияНоменклатуры
ИЗ
	ВТ_Остатки КАК ВремТЧ
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.НоменклатураПоставщика.СрезПоследних(
				&amp;ДатаОстатков,
				Номенклатура В
						(ВЫБРАТЬ
							ВремТЧ.Номенклатура
						ИЗ
							ВТ_Остатки КАК ВремТЧ)
					И Предприятие = &amp;Предприятие) КАК НоменклатураПоставщикаСрезПоследних
			ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ЦеныНоменклатуры.СрезПоследних(
					&amp;ДатаОстатков,
					Номенклатура В
						(ВЫБРАТЬ
							ВремТЧ.Номенклатура
						ИЗ
							ВТ_Остатки КАК ВремТЧ)) КАК ЦеныНоменклатурыСрезПоследних
			ПО НоменклатураПоставщикаСрезПоследних.Номенклатура = ЦеныНоменклатурыСрезПоследних.Номенклатура
		ПО ВремТЧ.Номенклатура = НоменклатураПоставщикаСрезПоследних.Номенклатура</query>
