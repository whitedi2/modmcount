	<Attributes>
		<Attribute name="Список" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ
	Д_ОбращенияВТехПоддержку.Дата КАК Дата,
	Д_ОбращенияВТехПоддержку.Ссылка,
	Д_ТекущаяЗадача.ТекущаяЗадача КАК Задача,
	Д_ТекущаяЗадача.Исполнители КАК Исполнитель,
	Д_ОбращенияВТехПоддержку.Автор,
	Д_ОбращенияВТехПоддержку.Номер,
	Д_ТекущаяЗадача.ТочкаМаршрута,
	ВЫБОР
		КОГДА Д_ТекущаяЗадача.ТочкаМаршрута = ЗНАЧЕНИЕ(БизнесПроцесс.СогласованиеОбщее.ТочкаМаршрута.Действие5)
			ТОГДА "НаИсполнении"
		ИНАЧЕ ВЫБОР
				КОГДА Д_ТекущаяЗадача.БизнесПроцессЗавершен = ИСТИНА
					ТОГДА "Исполнено"
				ИНАЧЕ ""
			КОНЕЦ
	КОНЕЦ КАК Стадия,
	ВЫБОР
		КОГДА (НЕ Д_ТекущаяЗадача.ТочкаМаршрута ЕСТЬ NULL 
					И НЕ Д_ТекущаяЗадача.ТочкаМаршрута = ЗНАЧЕНИЕ(БизнесПроцесс.СогласованиеОбщее.ТочкаМаршрута.Действие1)
					И НЕ Д_ТекущаяЗадача.ТочкаМаршрута = ЗНАЧЕНИЕ(БизнесПроцесс.СогласованиеОбщее.ТочкаМаршрута.Действие2)
				ИЛИ Д_ТекущаяЗадача.БизнесПроцессЗавершен = ИСТИНА)
				И НЕ Д_ТекущаяЗадача.ТекущаяЗадача = "На подготовке"
			ТОГДА ИСТИНА
		ИНАЧЕ ЛОЖЬ
	КОНЕЦ КАК Согласовано,
	ВЫБОР
		КОГДА (НЕ Д_ТекущаяЗадача.ТочкаМаршрута ЕСТЬ NULL 
					И НЕ Д_ТекущаяЗадача.ТочкаМаршрута = ЗНАЧЕНИЕ(БизнесПроцесс.СогласованиеОбщее.ТочкаМаршрута.Действие1)
					И НЕ Д_ТекущаяЗадача.ТочкаМаршрута = ЗНАЧЕНИЕ(БизнесПроцесс.СогласованиеОбщее.ТочкаМаршрута.Действие2)
					И НЕ Д_ТекущаяЗадача.ТочкаМаршрута = ЗНАЧЕНИЕ(БизнесПроцесс.СогласованиеОбщее.ТочкаМаршрута.Действие3)
				ИЛИ Д_ТекущаяЗадача.БизнесПроцессЗавершен = ИСТИНА)
				И НЕ Д_ТекущаяЗадача.ТекущаяЗадача = "На подготовке"
			ТОГДА ИСТИНА
		ИНАЧЕ ЛОЖЬ
	КОНЕЦ КАК Исполнено,
	ВЫБОР
		КОГДА (НЕ Д_ТекущаяЗадача.ТочкаМаршрута ЕСТЬ NULL 
					И НЕ Д_ТекущаяЗадача.ТочкаМаршрута = ЗНАЧЕНИЕ(БизнесПроцесс.СогласованиеОбщее.ТочкаМаршрута.Действие1)
					И НЕ Д_ТекущаяЗадача.ТочкаМаршрута = ЗНАЧЕНИЕ(БизнесПроцесс.СогласованиеОбщее.ТочкаМаршрута.Действие2)
					И НЕ Д_ТекущаяЗадача.ТочкаМаршрута = ЗНАЧЕНИЕ(БизнесПроцесс.СогласованиеОбщее.ТочкаМаршрута.Действие3)
					И НЕ Д_ТекущаяЗадача.ТочкаМаршрута = ЗНАЧЕНИЕ(БизнесПроцесс.СогласованиеОбщее.ТочкаМаршрута.Действие5)
				ИЛИ Д_ТекущаяЗадача.БизнесПроцессЗавершен = ИСТИНА)
				И НЕ Д_ТекущаяЗадача.ТекущаяЗадача = "На подготовке"
			ТОГДА ИСТИНА
		ИНАЧЕ ЛОЖЬ
	КОНЕЦ КАК Ознакомлено,
	Д_ОбращенияВТехПоддержку.ВидЗаявки,
	Д_ОбращенияВТехПоддержку.РазделПрограммы,
	Д_ОбращенияВТехПоддержку.ОтИмени,
	Д_ОбращенияВТехПоддержку.Проект,
	Д_ОбращенияВТехПоддержку.ПожеланиеКСроку,
	Д_ОбращенияВТехПоддержку.Содержание,
	Д_ОбращенияВТехПоддержку.Ответ,
	Д_ОбращенияВТехПоддержку.Примечания,
	Д_ОбращенияВТехПоддержку.ДатаНачалаРабот,
	Д_ОбращенияВТехПоддержку.СрокОкончанияРабот,
	Д_ОбращенияВТехПоддержку.ИсключитьИзОтчетаОПроделаннойРаботе,
	Д_ОбращенияВТехПоддержку.Важность,
	КОЛИЧЕСТВО(Д_ОбращенияВТехПоддержкуДетализацияРабот.НомерСтроки) КАК Подзадачи,
	КОЛИЧЕСТВО(Д_ОбращенияВТехПоддержкуДетализацияРабот.НомерСтроки) КАК Баги
ИЗ
	Документ.Д_ОбращенияВТехПоддержку КАК Д_ОбращенияВТехПоддержку
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.Д_ТекущаяЗадача КАК Д_ТекущаяЗадача
		ПО Д_ОбращенияВТехПоддержку.Ссылка = Д_ТекущаяЗадача.Документ
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			ВложенныйЗапрос.Документ КАК Документ
		ИЗ
			(ВЫБРАТЬ
				Д_ДоступностьДокументов.Документ КАК Документ
			ИЗ
				РегистрСведений.Д_ДоступностьДокументов КАК Д_ДоступностьДокументов
			ГДЕ
				(ВЫБОР
							КОГДА &amp;Согласовано
								ТОГДА ВЫРАЗИТЬ(Д_ДоступностьДокументов.Значение КАК Справочник.Пользователи) В (&amp;ТекущийПользователь)
										И Д_ДоступностьДокументов.Признак = "Согласование"
							ИНАЧЕ ЛОЖЬ
						КОНЕЦ
						ИЛИ ВЫБОР
							КОГДА &amp;Оплачено
								ТОГДА ВЫРАЗИТЬ(Д_ДоступностьДокументов.Значение КАК Справочник.Пользователи) В (&amp;ТекущийПользователь)
										И Д_ДоступностьДокументов.Признак = "Ознакомление"
							ИНАЧЕ ЛОЖЬ
						КОНЕЦ
						ИЛИ ВЫБОР
							КОГДА &amp;ПринадлежностьПредприятию
								ТОГДА ВЫБОР
										КОГДА &amp;ВсеПредприятия
											ТОГДА ИСТИНА
										ИНАЧЕ ВЫРАЗИТЬ(Д_ДоступностьДокументов.Значение КАК Справочник.Предприятия) В (&amp;ДоступныеПредприятия)
									КОНЕЦ
							ИНАЧЕ ЛОЖЬ
						КОНЕЦ
						ИЛИ ВЫБОР
							КОГДА &amp;Исполнено
								ТОГДА ВЫРАЗИТЬ(Д_ДоступностьДокументов.Значение КАК Справочник.Пользователи) В (&amp;ТекущийПользователь)
										И Д_ДоступностьДокументов.Признак = "Исполнение"
							ИНАЧЕ ЛОЖЬ
						КОНЕЦ
						ИЛИ ВЫБОР
							КОГДА &amp;ДопСтатьи
								ТОГДА ВЫРАЗИТЬ(Д_ДоступностьДокументов.Значение3 КАК Справочник.СтатьиДвиженияДенежныхСредств) В (&amp;ДоступныеСтатьиСырья)
										И ВЫБОР
											КОГДА &amp;ВсеПредприятия
												ТОГДА ИСТИНА
											ИНАЧЕ ВЫРАЗИТЬ(Д_ДоступностьДокументов.Значение КАК Справочник.Предприятия) В (&amp;ДоступныеПредприятия)
										КОНЕЦ
							ИНАЧЕ ЛОЖЬ
						КОНЕЦ)
				И ТИПЗНАЧЕНИЯ(Д_ДоступностьДокументов.Документ) = ТИП(Документ.Д_ОбращенияВТехПоддержку)
			
			ОБЪЕДИНИТЬ ВСЕ
			
			ВЫБРАТЬ
				Д_ОбращенияВТехПоддержку.Ссылка
			ИЗ
				Документ.Д_ОбращенияВТехПоддержку КАК Д_ОбращенияВТехПоддержку
			ГДЕ
				(ВЫБОР
							КОГДА &amp;Авторство
								ТОГДА Д_ОбращенияВТехПоддержку.Автор В (&amp;ТекущийПользователь)
							ИНАЧЕ ЛОЖЬ
						КОНЕЦ
						ИЛИ &amp;ВсеЗаявки)) КАК ВложенныйЗапрос
		
		СГРУППИРОВАТЬ ПО
			ВложенныйЗапрос.Документ) КАК ВложенныйЗапрос
		ПО Д_ОбращенияВТехПоддержку.Ссылка = ВложенныйЗапрос.Документ
		ЛЕВОЕ СОЕДИНЕНИЕ Документ.Д_ОбращенияВТехПоддержку.ДетализацияРабот КАК Д_ОбращенияВТехПоддержкуДетализацияРабот
