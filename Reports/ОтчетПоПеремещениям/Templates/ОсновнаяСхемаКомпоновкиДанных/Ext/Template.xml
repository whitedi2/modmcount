	ВТ_Перемещения.ДокОснование,
	ВТ_Перемещения.АвторПеремещение,
	ВТ_Поступления.АвторПоступления,
	ВТ_Поступления.Поступление,
	ВТ_Поступления.ВидОперацииПоступления,
	ВТ_Перемещения.Перемещение.Дата,
	ВТ_Поступления.Поступление.Дата,
	ВТ_Перемещения.ПодразделениеОтправитель,
	ВТ_Перемещения.СкладОтправитель,
	ВТ_Перемещения.ПодразделениеПолучатель,
	ВТ_Перемещения.СкладПолучатель,
	ВТ_Поступления.ПодразделениеОтправительПост,
	ВТ_Поступления.СкладОтправительПост,
	ВТ_Поступления.ПодразделениеПолучательПост,
	ВТ_Поступления.СкладПолучательПост
ИЗ
	ВТ_Перемещения КАК ВТ_Перемещения
		ПОЛНОЕ СОЕДИНЕНИЕ ВТ_Поступления КАК ВТ_Поступления
		ПО ВТ_Перемещения.Перемещение = ВТ_Поступления.ДокОснованиеПоступления</query>
