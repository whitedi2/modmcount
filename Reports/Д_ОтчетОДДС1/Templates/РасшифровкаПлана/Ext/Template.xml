		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ЕСТЬNULL(БюджетныйОбороты.СуммаОборот, 0) КАК План1,
	БюджетныйОбороты.Субконто1 КАК Статья,
	ВЫБОР
		КОГДА БюджетныйОбороты.Предприятия.УчетПоПодразделениям = ИСТИНА
			ТОГДА БюджетныйОбороты.Подразделение
		ИНАЧЕ "Основное"
	КОНЕЦ КАК Подразделение,
	БюджетныйОбороты.Предприятия КАК Предприятия,
	Бюджетный.Содержание КАК Содержание,
	Бюджетный.Регистратор КАК Документ,
	Бюджетный.Период КАК Дата,
	БюджетныйОбороты.Предприятия КАК Предприятие,
	БюджетныйОбороты.Субконто1 КАК Ссылка
ПОМЕСТИТЬ ВТ_Данные
ИЗ
	РегистрБухгалтерии.Бюджетный.Обороты(
			,
			,
			Запись,
			Счет В ИЕРАРХИИ (&amp;Счета),
			,
			(СценарийПлана = &amp;Сценарий
				ИЛИ СценарийПлана = &amp;СценарийВариант)
				И Субконто1 В ИЕРАРХИИ (&amp;Статья)
				И ВЫБОР
					КОГДА &amp;Предприятия = НЕОПРЕДЕЛЕНО
						ТОГДА ИСТИНА
					ИНАЧЕ Предприятия В (&amp;Предприятия)
				КОНЕЦ
				И ВЫБОР
					КОГДА &amp;Подразделение = "Основное"
							ИЛИ &amp;Подразделение = НЕОПРЕДЕЛЕНО
						ТОГДА ИСТИНА
					ИНАЧЕ Подразделение В (&amp;Подразделение)
				КОНЕЦ,
			,
			) КАК БюджетныйОбороты
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрБухгалтерии.Бюджетный КАК Бюджетный
		ПО БюджетныйОбороты.Регистратор = Бюджетный.Регистратор
			И БюджетныйОбороты.НомерСтроки = Бюджетный.НомерСтроки
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	СУММА(ВЫБОР
					КОГДА &amp;Сценарий = ЗНАЧЕНИЕ(Справочник.СценарииПланирования.Факт)
						ТОГДА ВТ_Данные.План1 * ЕСТЬNULL(КурсыВалютСрезПоследних.Курс, 1)
					ИНАЧЕ ВТ_Данные.План1 * ЕСТЬNULL(Б_ПлановыеКурсыВалютСрезПоследних1.Курс, 1)
		КОНЕЦ) КАК План1,
	ВТ_Данные.Статья КАК Статья,
	ВТ_Данные.Предприятия КАК Предприятия,
	ВТ_Данные.Подразделение КАК Подразделение,
	ВТ_Данные.Дата КАК Дата,
	ВТ_Данные.Документ КАК Документ,
	ВТ_Данные.Ссылка КАК Ссылка,
	ВТ_Данные.Предприятие КАК Предприятие,
	ВТ_Данные.Содержание КАК Содержание
ИЗ
	ВТ_Данные КАК ВТ_Данные
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.КурсыВалют.СрезПоследних(&amp;ДатаКурса {(&amp;ДатаКурса)}, БазоваяВалюта = &amp;ЭквивалентнаяВалюта) КАК КурсыВалютСрезПоследних
		ПО ВТ_Данные.Предприятия.ОсновнаяВалютаУчета = КурсыВалютСрезПоследних.Валюта
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.КурсыВалют.СрезПоследних(&amp;ДатаКурса {(&amp;ДатаКурса)}, Валюта = &amp;ЭквивалентнаяВалюта) КАК КурсЭквивалентнойВалюты
		ПО ВТ_Данные.Предприятия.ОсновнаяВалютаУчета = КурсЭквивалентнойВалюты.БазоваяВалюта
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.Б_ПлановыеКурсыВалют.СрезПоследних(
				&amp;ДатаКурсаБюджСц1 {(&amp;ДатаКурсаБюджСц1)},
				Сценарий = &amp;Сценарий
					И Валюта2 = &amp;ЭквивалентнаяВалюта) КАК Б_ПлановыеКурсыВалютСрезПоследних1
		ПО ВТ_Данные.Предприятия.ОсновнаяВалютаУчета = Б_ПлановыеКурсыВалютСрезПоследних1.Валюта1
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.Б_ПлановыеКурсыВалют.СрезПоследних(
				&amp;ДатаКурсаБюджСц1 {(&amp;ДатаКурсаБюджСц1)},
				Сценарий = &amp;Сценарий
					И Валюта1 = &amp;ЭквивалентнаяВалюта) КАК ПлановыйКурсЭквивалентнойВалюты1
		ПО ВТ_Данные.Предприятия.ОсновнаяВалютаУчета = ПлановыйКурсЭквивалентнойВалюты1.Валюта2

СГРУППИРОВАТЬ ПО
	ВТ_Данные.Статья,
	ВТ_Данные.Предприятие,
	ВТ_Данные.Подразделение,
	ВТ_Данные.Дата,
	ВТ_Данные.Документ,
	ВТ_Данные.Ссылка,
	ВТ_Данные.Предприятия,
	ВТ_Данные.Содержание</query>
