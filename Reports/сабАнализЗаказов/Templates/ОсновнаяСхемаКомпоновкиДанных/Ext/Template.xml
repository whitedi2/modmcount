		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ЗаказКлиентаТабличнаяЧасть.Ссылка КАК Ссылка,
	СУММА(1) КАК КоличествоПозиций
ПОМЕСТИТЬ ВремПозицииЗаказа
ИЗ
	Документ.ЗаказКлиента.ТабличнаяЧасть КАК ЗаказКлиентаТабличнаяЧасть
ГДЕ
	ЗаказКлиентаТабличнаяЧасть.Ссылка.Дата МЕЖДУ &amp;Дата1 И &amp;Дата2
	И ЗаказКлиентаТабличнаяЧасть.Ссылка.Проведен = ИСТИНА

СГРУППИРОВАТЬ ПО
	ЗаказКлиентаТабличнаяЧасть.Ссылка
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ЗаказКлиента.Ссылка КАК Заказ,
	ЗаказКлиента.Контрагент КАК Контрагент,
	ЗаказКлиента.ПодразделениеКонтрагента КАК ПодразделениеКонтрагента,
	ЗаказКлиента.Дата КАК Дата,
	1 КАК Количество,
	ЗаказКлиента.ВесТовара КАК ВесТовара,
	ЗаказКлиента.Ответственный КАК Ответственный,
	ЗаказКлиента.СуммаДокумента КАК СуммаДокумента,
	ЕСТЬNULL(ВремПозицииЗаказа.КоличествоПозиций, 0) КАК КоличествоПозиций
ИЗ
	Документ.ЗаказКлиента КАК ЗаказКлиента
		ЛЕВОЕ СОЕДИНЕНИЕ ВремПозицииЗаказа КАК ВремПозицииЗаказа
		ПО ЗаказКлиента.Ссылка = ВремПозицииЗаказа.Ссылка
ГДЕ
	ЗаказКлиента.Дата МЕЖДУ &amp;Дата1 И &amp;Дата2
	И ЗаказКлиента.Проведен = ИСТИНА</query>
