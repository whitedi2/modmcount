		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	УчетныйОбороты.Субконто1 КАК Статья,
	УчетныйОбороты.Предприятия КАК Предприятие,
	ВЫРАЗИТЬ(УчетныйОбороты.Субконто1 КАК Справочник.СтатьиДвиженияДенежныхСредств).ВидДеятельности КАК ВидДеятельности,
	СУММА(ЕСТЬNULL(УчетныйОбороты.СуммаОборот, 0)) КАК ФактРеал,
	УчетныйОбороты.Подразделение КАК Подразделение,
	NULL КАК Поле1,
	NULL КАК Поле2,
	УчетныйОбороты.Период,
	УчетныйОбороты.Регистратор,
	УчетныйОбороты.Счет,
	УчетныйОбороты.КорСчет
ПОМЕСТИТЬ ВТ_Данные
ИЗ
	РегистрБухгалтерии.Учетный.Обороты(
			&amp;ДатаНач,
			&amp;ДатаКон,
			Запись,
			Счет В ИЕРАРХИИ (ЗНАЧЕНИЕ(ПланСчетов.Учетный.Касса))
				ИЛИ Счет В ИЕРАРХИИ (ЗНАЧЕНИЕ(ПланСчетов.Учетный.РасчетныйСчет))
				ИЛИ Счет В ИЕРАРХИИ (ЗНАЧЕНИЕ(ПланСчетов.Учетный.КазнаОсн)),
			,
			Предприятия В (&amp;Предприятие)
				И НЕ Предприятия = &amp;Отель,
			,
			) КАК УчетныйОбороты

СГРУППИРОВАТЬ ПО
	УчетныйОбороты.Субконто1,
	УчетныйОбороты.Предприятия,
	ВЫРАЗИТЬ(УчетныйОбороты.Субконто1 КАК Справочник.СтатьиДвиженияДенежныхСредств).ВидДеятельности,
	УчетныйОбороты.Подразделение,
	УчетныйОбороты.Период,
	УчетныйОбороты.Регистратор,
	УчетныйОбороты.Счет,
	УчетныйОбороты.КорСчет

ОБЪЕДИНИТЬ ВСЕ


ВЫБРАТЬ
	УчетныйОбороты.Субконто1,
	УчетныйОбороты.Предприятия,
	ВЫРАЗИТЬ(УчетныйОбороты.Субконто1 КАК Справочник.СтатьиДвиженияДенежныхСредств).ВидДеятельности,
	СУММА(ЕСТЬNULL(УчетныйОбороты.СуммаОборот, 0)),
	УчетныйОбороты.Подразделение,
	NULL,
	NULL,
	УчетныйОбороты.Период,
	УчетныйОбороты.Регистратор,
	УчетныйОбороты.Счет,
	УчетныйОбороты.КорСчет
ИЗ
	РегистрБухгалтерии.Учетный.Обороты(
			&amp;ДатаНач,
			&amp;ДатаКон,
			Запись,
			Счет В ИЕРАРХИИ (ЗНАЧЕНИЕ(ПланСчетов.Учетный.Касса))
				ИЛИ Счет В ИЕРАРХИИ (ЗНАЧЕНИЕ(ПланСчетов.Учетный.РасчетныйСчет)),
			,
			Предприятия В (&amp;Предприятие)
				И Предприятия = &amp;Отель,
			,
			) КАК УчетныйОбороты

СГРУППИРОВАТЬ ПО
	УчетныйОбороты.Субконто1,
	УчетныйОбороты.Предприятия,
	ВЫРАЗИТЬ(УчетныйОбороты.Субконто1 КАК Справочник.СтатьиДвиженияДенежныхСредств).ВидДеятельности,
	УчетныйОбороты.Подразделение,
	УчетныйОбороты.КорСчет,
	УчетныйОбороты.Период,
	УчетныйОбороты.Регистратор,
	УчетныйОбороты.Счет

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	УчетныйОбороты.Субконто2,
	УчетныйОбороты.Предприятия,
	ВЫРАЗИТЬ(УчетныйОбороты.Субконто2 КАК Справочник.СтатьиДвиженияДенежныхСредств).ВидДеятельности,
	СУММА(ЕСТЬNULL(УчетныйОбороты.СуммаОборот, 0)),
	УчетныйОбороты.Подразделение,
	NULL,
	NULL,
	УчетныйОбороты.Период,
	УчетныйОбороты.Регистратор,
	УчетныйОбороты.Счет,
	УчетныйОбороты.КорСчет
ИЗ
	РегистрБухгалтерии.Учетный.Обороты(
			&amp;ДатаНач,
			&amp;ДатаКон,
			Запись,
			Счет В ИЕРАРХИИ (ЗНАЧЕНИЕ(ПланСчетов.Учетный.КраткосрочныеКредиты)),
			,
			Предприятия В (&amp;Предприятие)
				И Предприятия = &amp;Отель,
			,
			) КАК УчетныйОбороты

СГРУППИРОВАТЬ ПО
	УчетныйОбороты.Субконто2,
	УчетныйОбороты.Предприятия,
	ВЫРАЗИТЬ(УчетныйОбороты.Субконто2 КАК Справочник.СтатьиДвиженияДенежныхСредств).ВидДеятельности,
	УчетныйОбороты.Подразделение,
	УчетныйОбороты.Период,
	УчетныйОбороты.Регистратор,
	УчетныйОбороты.КорСчет,
	УчетныйОбороты.Счет
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ВТ_Данные.Предприятие,
	МАКСИМУМ(КроссКурсыВалютСрезПоследних.Курс) КАК КроссКурсФ,
	МАКСИМУМ(КурсыВалютСрезПоследних.Курс) КАК КурсФ,
	МАКСИМУМ(КурсЭквивалентнойВалюты.Курс) КАК КурсЭквФ
ПОМЕСТИТЬ ВТ_Курсы
ИЗ
	ВТ_Данные КАК ВТ_Данные
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.КроссКурсыВалют.СрезПоследних(&amp;ДатаКурса {(&amp;ДатаКурса)}, Валюта2 = &amp;ЭквивалентнаяВалюта) КАК КроссКурсыВалютСрезПоследних
		ПО ВТ_Данные.Предприятие.ОсновнаяВалютаУчета = КроссКурсыВалютСрезПоследних.Валюта1
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.КурсыВалют.СрезПоследних(&amp;ДатаКурса {(&amp;ДатаКурса)}, ) КАК КурсыВалютСрезПоследних
		ПО ВТ_Данные.Предприятие.ОсновнаяВалютаУчета = КурсыВалютСрезПоследних.Валюта,
	РегистрСведений.КурсыВалют.СрезПоследних(&amp;ДатаКурса {(&amp;ДатаКурса)}, Валюта = &amp;ЭквивалентнаяВалюта) КАК КурсЭквивалентнойВалюты

СГРУППИРОВАТЬ ПО
	ВТ_Данные.Предприятие
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ВТ_Данные.Статья,
	ВТ_Данные.Предприятие,
	ВТ_Данные.ВидДеятельности,
	ВТ_Данные.Подразделение,
	ВТ_Данные.ФактРеал * ЕСТЬNULL(ВТ_Курсы.КурсФ, 1) КАК ФактРеал,
	ВТ_Данные.Период,
	ВТ_Данные.Регистратор,
	ВТ_Данные.Счет,
	ВТ_Данные.КорСчет
ПОМЕСТИТЬ ВТ_ДанныеВалюта
ИЗ
	ВТ_Данные КАК ВТ_Данные
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_Курсы КАК ВТ_Курсы
		ПО ВТ_Данные.Предприятие = ВТ_Курсы.Предприятие
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_ДанныеВалюта.Статья,
	ВТ_ДанныеВалюта.Предприятие КАК Предприятие,
	ВТ_ДанныеВалюта.ВидДеятельности,
	ВТ_ДанныеВалюта.Подразделение,
	ВТ_ДанныеВалюта.ФактРеал,
	ВТ_ДанныеВалюта.Период,
	ВТ_ДанныеВалюта.Регистратор,
	ВТ_ДанныеВалюта.Счет,
	ВТ_ДанныеВалюта.КорСчет,
	ВЫБОР
		КОГДА ВТ_ДанныеВалюта.Статья.Доход
			ТОГДА ЕСТЬNULL(ВТ_ДанныеВалюта.ФактРеал, 0)
		ИНАЧЕ ЕСТЬNULL(-ВТ_ДанныеВалюта.ФактРеал, 0)
	КОНЕЦ КАК Факт
ИЗ
	ВТ_ДанныеВалюта КАК ВТ_ДанныеВалюта
ГДЕ
	НЕ ВТ_ДанныеВалюта.Подразделение В (&amp;ПодразделенияДляАО)</query>
	</dataSet>
	<totalField>
		<dataPath>Факт</dataPath>
