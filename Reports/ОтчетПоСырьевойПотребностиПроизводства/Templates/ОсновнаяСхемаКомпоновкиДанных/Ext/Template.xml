		<field xsi:type="DataSetFieldField">
			<dataPath>КоличествоМатериалы</dataPath>
			<field>КоличествоМатериалы</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Потребность</v8:content>
				</v8:item>
