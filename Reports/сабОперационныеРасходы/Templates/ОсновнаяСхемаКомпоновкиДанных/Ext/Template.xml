		<query>ВЫБРАТЬ
	УчетныйОстаткиИОбороты.Предприятия КАК Предприятие,
	УчетныйОстаткиИОбороты.Подразделение КАК Подразделение,
	УчетныйОстаткиИОбороты.ПериодМесяц КАК ПериодМесяц,
	СУММА(УчетныйОстаткиИОбороты.СуммаОборотКт) КАК Выручка
ПОМЕСТИТЬ ВТВыручка
ИЗ
	РегистрБухгалтерии.Учетный.ОстаткиИОбороты({(НАЧАЛОПЕРИОДА(&amp;НачалоПериода, ДЕНЬ))}, {(КОНЕЦПЕРИОДА(&amp;КонецПериода, ДЕНЬ))}, Авто, , Счет.Код = "90.01", , ) КАК УчетныйОстаткиИОбороты

СГРУППИРОВАТЬ ПО
	УчетныйОстаткиИОбороты.Предприятия,
	УчетныйОстаткиИОбороты.Подразделение,
	УчетныйОстаткиИОбороты.ПериодМесяц
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	УчетныйОстаткиИОбороты.ПериодМесяц КАК ПериодМесяц,
	УчетныйОстаткиИОбороты.Регистратор КАК Регистратор,
	ВЫБОР
		КОГДА ВЫРАЗИТЬ(УчетныйОстаткиИОбороты.Субконто1 КАК Справочник.СтатьиЗатрат).Ссылка ЕСТЬ NULL
			ТОГДА ВЫБОР
					КОГДА ВЫРАЗИТЬ(УчетныйОстаткиИОбороты.Субконто2 КАК Справочник.СтатьиЗатрат).Ссылка ЕСТЬ NULL
						ТОГДА ВЫБОР
								КОГДА ВЫРАЗИТЬ(УчетныйОстаткиИОбороты.Субконто3 КАК Справочник.СтатьиЗатрат).Ссылка ЕСТЬ NULL
									ТОГДА "..."
								ИНАЧЕ УчетныйОстаткиИОбороты.Субконто3
							КОНЕЦ
					ИНАЧЕ УчетныйОстаткиИОбороты.Субконто2
				КОНЕЦ
		ИНАЧЕ УчетныйОстаткиИОбороты.Субконто1
	КОНЕЦ КАК СтатьяЗатрат,
	ВЫБОР
		КОГДА ВЫРАЗИТЬ(УчетныйОстаткиИОбороты.Субконто1 КАК Справочник.СтатьиЗатрат).Ссылка ЕСТЬ NULL
			ТОГДА ВЫБОР
					КОГДА ВЫРАЗИТЬ(УчетныйОстаткиИОбороты.Субконто2 КАК Справочник.СтатьиЗатрат).Ссылка ЕСТЬ NULL
						ТОГДА ВЫБОР
								КОГДА ВЫРАЗИТЬ(УчетныйОстаткиИОбороты.Субконто3 КАК Справочник.СтатьиЗатрат).Ссылка ЕСТЬ NULL
									ТОГДА "..."
								ИНАЧЕ ВЫРАЗИТЬ(УчетныйОстаткиИОбороты.Субконто3 КАК Справочник.СтатьиЗатрат).Родитель
							КОНЕЦ
					ИНАЧЕ ВЫРАЗИТЬ(УчетныйОстаткиИОбороты.Субконто2 КАК Справочник.СтатьиЗатрат).Родитель
				КОНЕЦ
		ИНАЧЕ ВЫРАЗИТЬ(УчетныйОстаткиИОбороты.Субконто1 КАК Справочник.СтатьиЗатрат).Родитель
	КОНЕЦ КАК ГруппаЗатрат,
	УчетныйОстаткиИОбороты.Предприятия КАК Предприятие,
	УчетныйОстаткиИОбороты.Подразделение КАК Подразделение,
	УчетныйОстаткиИОбороты.СуммаОборот КАК СуммаОборот,
	УчетныйОстаткиИОбороты.СценарийПлана КАК СценарийПлана
ПОМЕСТИТЬ ВТОбороты
ИЗ
	РегистрБухгалтерии.Учетный.ОстаткиИОбороты(
			{(НАЧАЛОПЕРИОДА(&amp;НачалоПериода, ДЕНЬ))},
			{(КОНЕЦПЕРИОДА(&amp;КонецПериода, ДЕНЬ))},
			Авто,
			,
			Счет В ИЕРАРХИИ (&amp;Счет20)
				ИЛИ ВЫБОР
					КОГДА &amp;ЕСТЬ23
						ТОГДА Счет В ИЕРАРХИИ (&amp;Счет23)
					ИНАЧЕ ЛОЖЬ
				КОНЕЦ
				ИЛИ Счет В ИЕРАРХИИ (&amp;Счет25),
			,
			{(Подразделение = &amp;Подразделение), (СценарийПлана = &amp;Сценарий1)}) КАК УчетныйОстаткиИОбороты
ГДЕ
	ТИПЗНАЧЕНИЯ(УчетныйОстаткиИОбороты.Регистратор) &lt;&gt; ТИП(Документ.УЧ_ЗакрытиеПериода)
	
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТОбороты.СтатьяЗатрат КАК СтатьяЗатрат,
	ВТОбороты.СтатьяЗатрат.Наименование КАК СтатьяЗатратНаименование,
	ВТОбороты.Предприятие КАК Предприятие,
	ВТОбороты.Подразделение КАК Подразделение,
	ВТОбороты.ПериодМесяц КАК ПериодМесяц,
	ВТОбороты.ГруппаЗатрат КАК ГруппаЗатрат,
	ЕСТЬNULL(ВТОбороты.СуммаОборот, 0) КАК СуммаОборот,
	ВТОбороты.СценарийПлана КАК СценарийПлана
ПОМЕСТИТЬ ВТРезульт
ИЗ
	ВТОбороты КАК ВТОбороты

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	NULL,
	"Выручка",
	NULL,
	NULL,
	ЕСТЬNULL(ВТОбороты.ПериодМесяц,ВТВыручка.ПериодМесяц),
	NULL,
	СРЕДНЕЕ(ЕСТЬNULL(ВТВыручка.Выручка, 0)),
	NULL
ИЗ
	ВТОбороты КАК ВТОбороты
		ПОЛНОЕ СОЕДИНЕНИЕ ВТВыручка КАК ВТВыручка
		ПО ВТОбороты.ПериодМесяц = ВТВыручка.ПериодМесяц
			//И ВТОбороты.Предприятие = ВТВыручка.Предприятие

СГРУППИРОВАТЬ ПО
	ЕСТЬNULL(ВТОбороты.ПериодМесяц,ВТВыручка.ПериодМесяц)

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	NULL,
	"Рентабельность",
	NULL,
	NULL,
	ВТОбороты.ПериодМесяц,
	NULL,
	ВЫРАЗИТЬ(ВЫБОР
			КОГДА СРЕДНЕЕ(ЕСТЬNULL(ВТВыручка.Выручка, 0)) = 0
				ТОГДА 0
			ИНАЧЕ СУММА(ЕСТЬNULL(ВТОбороты.СуммаОборот, 0)) / СРЕДНЕЕ(ЕСТЬNULL(ВТВыручка.Выручка, 0)) * 100
		КОНЕЦ КАК ЧИСЛО(15, 2)),
	NULL
ИЗ
	ВТОбороты КАК ВТОбороты
		ЛЕВОЕ СОЕДИНЕНИЕ ВТВыручка КАК ВТВыручка
		ПО ВТОбороты.ПериодМесяц = ВТВыручка.ПериодМесяц
			И ВТОбороты.Предприятие = ВТВыручка.Предприятие

СГРУППИРОВАТЬ ПО
	ВТОбороты.ПериодМесяц
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТРезульт.СтатьяЗатрат КАК СтатьяЗатрат,
	ВТРезульт.СтатьяЗатратНаименование КАК СтатьяЗатратНаименование,
	ВТРезульт.Предприятие КАК Предприятие,
	ВТРезульт.Подразделение КАК Подразделение,
	ВТРезульт.ПериодМесяц КАК ПериодМесяц,
	ВТРезульт.СуммаОборот КАК СуммаОборот,
	ВТРезульт.СценарийПлана КАК СценарийПлана,
	ВТРезульт.ГруппаЗатрат КАК ГруппаЗатрат
{ВЫБРАТЬ
	СценарийПлана.*,
	СтатьяЗатрат.*,
	Предприятие.*,
	Подразделение.*,
	ГруппаЗатрат.*,
	ПериодМесяц,
	СуммаОборот}
ИЗ
	ВТРезульт КАК ВТРезульт
{ГДЕ
	(ВЫБОР
			КОГДА ВТРезульт.СтатьяЗатратНаименование = "Рентабельность"
					ИЛИ ВТРезульт.СтатьяЗатратНаименование = "Выручка"
				ТОГДА ИСТИНА
			ИНАЧЕ ВТРезульт.Подразделение = &amp;Подразделение
		КОНЕЦ),
	(ВЫБОР
			КОГДА ВТРезульт.СтатьяЗатратНаименование = "Рентабельность"
					ИЛИ ВТРезульт.СтатьяЗатратНаименование = "Выручка"
				ТОГДА ИСТИНА
			ИНАЧЕ ВТРезульт.СценарийПлана = &amp;Сценарий1
		КОНЕЦ),
	ВТРезульт.СтатьяЗатрат.*,
	ВТРезульт.Предприятие.*,
	ВТРезульт.Подразделение.*,
	ВТРезульт.ГруппаЗатрат.*,
	ВТРезульт.ПериодМесяц,
	ВТРезульт.СуммаОборот}</query>
	</dataSet>
